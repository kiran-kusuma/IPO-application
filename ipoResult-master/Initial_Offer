     $(document).ready(function() {
         $('.network-slider').slick({
             autoplay: true,
             autoplaySpeed: 5000,
             speed: 500,
             dots: true,
             prevArrow: false,
             nextArrow: false,
             infinite: true,
             slidesToShow: 1,
             slidesToScroll: 1
         });
         $("#txtStat").prop("placeholder", "Enter PAN").val("").focus().blur();
         $("#txtStat").attr("maxlength", 10);
         $("#hdnval").val("1");
         $('#txtStat').keyup(function() {
             var $th = $(this);
this.value=this.value.toUpperCase();
             $th.val($th.val().replace(/[^a-zA-Z0-9]/g, function(str) { ShowCustomDialog('You typed " ' + str + ' ".\n\nPlease use only letters and numbers.');  return ''; }));
         });
     });
    </script>
<script type="text/javascript" language="javascript">

        function CheckingCaptcha() { var var_ret = "0"; $.ajax({ type: "POST", url: "CaptchaImage.aspx/CheckCaptcha", data: "{}", contentType: "application/json; charset=utf-8", dataType: "json", async: false, success: function(response) { var_ret = response.d; }, failure: function(msg) { var_ret = "0"; } }); return var_ret; }
        function enablebtn() {
            $("#btnsearc").removeAttr("disabled");
        }
        


        $(document).ready(function() {
        GenToken();
         $('#chknextAppln').click(function() {
                $("#btnsearc").removeAttr("disabled");
                $("#tbl_DetSec").hide();
                $('#chknextAppln').hide();
                $('#divupdatebnk').hide();                
                $("body").scrollTop(0);
		$('input:radio[id=349]').prop('checked', true);
                $("#txtStat").prop("placeholder", "Enter PAN").val("").focus().blur();
                $("#txtStat").attr("maxlength", 10);
                $("#hdnval").val("1");
            });   
            $('#txtStat').focus(function() {
                $("#btnsearc").removeAttr("disabled");
                $("#tbl_DetSec").hide();
            });
            $('#img_cap').bind('contextmenu', function(e) {
                return false;
            });
            $('#txtCaptch').bind('contextmenu', function(e) {
                return false;
            });
            $('#txtCaptch').bind("cut copy paste", function(e) {
                e.preventDefault();
            });
     });
    function CheckingCaptcha() 
    {
        var var_ret = "0"; 
        $.ajax({ 
            type: "POST", 
            url: "CaptchaImage.aspx/CheckCaptcha", 
            //data: "{}", 
            contentType: "application/json; charset=utf-8", 
            dataType: "json", 
            async: false,         
            success: function(response) 
            { 
                var_ret = response.d; 
            }, 
            failure: function(msg) 
            { 
                var_ret = "0"; 
            } 
        }); 
        return var_ret; 
    }
    function enablebtn() {
        $("#btnsearc").removeAttr("disabled");
    }
    $(function Details() 
    {
        $("#tbl_DetSec").hide();
        $.ajax({
            type: "POST",
            url: "IPO.aspx/GetDetails",
            contentType: "application/json;charset:utf-8",
            dataType: "json",
            async:false,
            success: refreshDataBind,
            failure: function(response) {
                alert(response.d);
            },
            error: function(xhr, status, error) {
                var err = eval("(" + xhr.responseText + ")");
                alert(err.Message);
            }
        });
    });
    function refreshDataBind(response) 
    {
        var xmlDoc = $.parseXML(response.d);
        var xml = $(xmlDoc);
        var customers = xml.find("Table");
        //$("#tbl_detsec2").hide();
        $("#ddlCompany").empty();
        $("#ddlCompany").append($("<option></option>").val(0).html('----Select Company----'));
        if (customers.length > 0) {             
            $.each(customers, function() {
            $("#ddlCompany").append($("<option></option>").val($(this).find("company_id").text()).html($(this).find("companyname").text()));
        });
        }
    }
    var characterReg = /^\s*[a-zA-Z0-9,\s]+\s*$/;
    function CALLPANSERCH() {
    
        $("#btnsearc").attr("disabled", "disabled");
        var appnum;
        var ifscCode;
        appnum = $('#txtStat').val();
        appnum = appnum.trim();
        ifscCode = $('#txtIfsc').val();
        ifscCode = ifscCode.trim();
        var chkVal = document.getElementById("hdnval").value;
        var value = CheckingCaptcha();
        if ($('#ddlCompany').val() == 0) {
            ShowCustomDialog('Please Select Company');
                //refreshCaptcha();
            enablebtn();
            return false;
        }
        if (chkVal == 1) {            
            if (appnum == '' || appnum == "Enter PAN") {
                ShowCustomDialog('PAN can not be left blank');
                    //refreshCaptcha();
                enablebtn();
                return false;
            }
            else if (appnum.length != 10) {
                ShowCustomDialog('Enter Valid PAN');
                    //refreshCaptcha();
                enablebtn();
                return false;
            }
        }
        else if (chkVal == 2) {
            if (appnum == '' || appnum == "Enter Application Number") {
                ShowCustomDialog('Application Number can not be left blank');
                //refreshCaptcha();
                enablebtn();
                return false;
            }
        }
        else if (chkVal == 3) {
            if (appnum == '' || appnum == "Enter DPID CLID") {
                ShowCustomDialog('DP/Client ID can not be left blank');
                    //refreshCaptcha();
                enablebtn();
                return false;
            }
            else if (appnum.length != 16) {
                ShowCustomDialog('Enter Valid DP/Client ID');
                    //refreshCaptcha();
                enablebtn();
                return false;
            }
        } 
        else if (chkVal == 4) {
            if (appnum == '' || appnum == "Enter Account No") {
                ShowCustomDialog('Account No can not be left blank');
               // refreshCaptcha();
                enablebtn();
                $('#txtStat').focus();
                return false;                    
              }
            if (ifscCode == '' || ifscCode == "Enter IFSC Code") {
                ShowCustomDialog('IFSC code can not be left blank');
                //refreshCaptcha();
                enablebtn();
                $('#txtIfsc').focus();
                return false;                    
            }
        }
        var captchacahr = $('#txtCaptch').val();
        if ($('#ddlCompany').val() == 0) {
            ShowCustomDialog('Please Select IPO');
                //refreshCaptcha();
            enablebtn();
            return false;
        }
        else if (appnum.length > 0 && characterReg.test(appnum) == false) {
            ShowCustomDialog('No special characters allowed.');
                //refreshCaptcha();
            enablebtn();
            return false;
        }
//            else if (captchacahr == '' || captchacahr == "Enter Captcha") {

//                ShowCustomDialog('Enter Valid Captcha Letters');
//            //refreshCaptcha();
//            enablebtn();
//            return false;
//            }
//            else if (captchacahr != value) {

//                ShowCustomDialog('Enter Valid Captcha');
//            refreshCaptcha();
//            enablebtn();
//            return false;
//            }
        else {
        
            $.ajax({
                type: "POST",
                url: "IPO.aspx/SearchOnPan",
                //data: "{'MODE': '" + mode + "'}",
                //data: "{'clientid': '" + $('#ddlCompany').val() + "','PAN': '" + $('#txtStat').val() + "','CHKVAL': '" + $('#hdnval').val() + "'}",
                //data: "{'clientid': '" + $('#ddlCompany').val() + "','PAN': '" + $('#txtStat').val() + "','IFSC': '" + ifscCode + "','CHKVAL': '" + $('#hdnval').val() + "'}",
                data: "{'clientid': '" + $('#ddlCompany').val() + "','PAN': '" + $('#txtStat').val() + "','IFSC': '" + ifscCode + "','CHKVAL': '" + $('#hdnval').val() + "','token': '" + $('#hidToken').val() + "'}",
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                async:false,
                //success: OnSuccess,
                success: onDataBindPAN,
                failure: function(response) {
                    alert("failure");
                    alert(response.d);
                },
                error: function(response) {
                    alert(response.d);
                }
            });
        }
    }
    function onDataBindPAN(response) 
    {
        var xmlDoc = $.parseXML(response.d);
        var xml = $(xmlDoc);
        var maindiv;
        var submaindiv;
        var loaderdiv;
        var sliderdiv;
        var navigatordiv;
        var alt_count_color = 0;
        var astyle;
        var pan = "";
        var pan_old = "";
        var _flag_msg=0;
        /* new design code starts from here */
        $('#txtCaptch').val("Enter Captcha");
        var firstlevelchilds = xml.find("Table");
        var firstlevelchilds1 = xml.find("Table1");
        var _COUNTROW=0
        var _COUNTROW1=0
        var inner_content = "";
        
        
         if (firstlevelchilds1.length > 0) {
            //COUNTROW=firstlevelchilds.length
            $.each(firstlevelchilds1, function() 
            {
               
              var msg=$(this).find("Msg").text();
              _flag_msg=1;
                alert(msg);
                GenToken();
                return false;
                
            });
            
        }
        
        
        
        
        
        
        
        
        if(_flag_msg==0)
        {
        
        
        if (firstlevelchilds.length > 0) {
            COUNTROW=firstlevelchilds.length
            $.each(firstlevelchilds, function() 
            {
                $("#tbl_DetSec").show();
                $("#tbl_DetSec").empty();
                // $("#tbl_detsec1").show();
                var roundAmt = $(this).find("RFNDAMT").text(); // Convert.ToInt32(ds.Tables[0].Rows[0]["RFNDAMT"]);
                var pull = $(this).find("pull").text(); // ds.Tables[0].Rows[0]["pull"].ToString();
                var match = $(this).find("match").text(); //ds.Tables[0].Rows[0]["match"].ToString();
                var allot = $(this).find("ALLOT").text(); //Convert.ToInt32(ds.Tables[0].Rows[0]["ALLOT"]);   
                inner_content=inner_content+"<table  border='1'  style='padding:0!important;box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0 rgba(0, 0, 0, 0.19); margin:30px 0; width:100%;'>";
                inner_content=inner_content+"<tr class='heading_table'><th colspan='4' ><span >Application Details - "+$(this).find("PEMNDG").text()+"</span></th></tr>";
                inner_content=inner_content+"<tr><td class='table_data_name' >Sole / 1st Applicant</td><td colspan='4' style='color: #F26729;' class='table_data'  >"+$(this).find("NAME1").text()+"</td></tr>";
                //inner_content=inner_content+"<tr><td  >Sole / 1st Applicant</td><td colspan='4'  >"+$(this).find("NAME1").text()+"</td></tr>"
                inner_content=inner_content+"<tr><td >Securities applied</td><td style='color: #F26729;' class='table_data'>"+$(this).find("SHARES").text()+"</td><td >Cut off Price</td><td style='color: #F26729;' class='table_data' >"+$(this).find("offer_price").text()+"</td></tr>";
                
               
                         inner_content=inner_content+"<tr><td>Securities Allotted</td><td style='color: #F26729;' class='table_data' >"+$(this).find("ALLOT").text()+"</td><td >Amount Adjusted</td><td  style='color: #F26729;' class='table_data'>"+$(this).find("AMTADJ").text()+"</td></tr>";
                 if ($('#ddlCompany').val()=='11657')
{
                    inner_content=inner_content+"<tr><td colspan='4'>Price on Application (50%) : Rs 1,638 per FPO Equity Share and an upfront discount of Rs. 64 per FPO Equity Share was offered to Retail Individual Bidders.</td></tr>";
                   }
                    
                   if ($(this).find("INVCODE").text()==91 && $(this).find("BNKCODE").text()==0 )
                    {
                         inner_content=inner_content+"<tr><td colspan='4'><span style='color:red'>*</span>Application bidded but amount not blocked</td></tr>";
                    }
                   
                
                
                
                inner_content=inner_content+"</table>";
                _COUNTROW1=_COUNTROW1+1;
            });
            $("#chknextAppln").show()
            $("#tbl_DetSec").append(inner_content);
            GenToken();
            //refreshCaptcha();
            enablebtn();
            var scrollTo = $('#btnsearc');
            var container = $('html, body');            
            container.animate({
                scrollTop: scrollTo.offset().top - container.scrollTop()
            },1000);
        }
        else 
        {
            ShowCustomDialog('No Record Found!!');
                //refreshCaptcha();
                GenToken();
            enablebtn();
        }
        }
    }
    function onDataBindClient(response) 
    {
        var xmlDoc = $.parseXML(response.d);
        var xml = $(xmlDoc);
        var maindiv;
        var submaindiv;
        var loaderdiv;
        var sliderdiv;
        var navigatordiv;
        var alt_count_color = 0;
        var astyle;
        var pan = "";
        var pan_old = "";
        /* new design code starts from here */
        $('#txtCaptch').val("Enter Captcha");
        var firstlevelchilds = xml.find("Table");
        var inner_detail;
        if (firstlevelchilds.length > 0) {
            $.each(firstlevelchilds, function() 
            {                    
                inner_detail="<a href='mailto:"+$(this).find("InvestorEmail").text()+"'>"+$(this).find("InvestorEmail").text()+"</a>";                  
                $('#td_email').html(inner_detail);
                enablebtn();
            });
        }
        else 
        {
            inner_detail = "<a href='mailto:ipo.helpdesk@linkintime.co.in'>ipo.helpdesk@linkintime.co.in</a>";                  
            $('#td_email').html(inner_detail);                
                //refreshCaptcha();
            enablebtn();
        }
        BindBasicOFAllotment()
        
    }
    $(function() {
        $('#ddlCompany').change(function() {
	
		$('#txtStat').val("");
                $('#txtIfsc').val("");
		

            var drop_down_1_value = $('#ddlCompany').val();
            $('#ddlCompany').val();
            $("#tbl_DetSec").hide();
            if (drop_down_1_value == 0) {
                $('#td_email').html('<a href="mailto:ipo.helpdesk@linkintime.co.in">ipo.helpdesk@linkintime.co.in</a>');
            }
            else 
            {                
                $(this).find('option').css('color', '#000'); $(this).find('option:selected').css('color', '#FE651F');
                $.ajax({
                    type: "POST",
                    url: "IPO.aspx/GetDetails_CLT",
                    //data: "{'MODE': '" + mode + "'}",
                    data: "{'CLIENTID': '" + $('#ddlCompany').val() + "'}",
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    async:false,
                    //success: OnSuccess,
                    success: onDataBindClient,
                    failure: function(response) {
                        alert("failure");
                        alert(response.d);
                    },
                    error: function(response) {                            
                        alert(response.d);
                    }                    
                });
            }

        });
    });
    function refreshCaptcha() {
        $('#txtCaptch').val("Enter Captcha");
        $.ajax({
            url: '\CImage.aspx',
            async:false,
            processData: false
        }).always(function(b64data) {
            $("#img_cap").attr('src', '');
            var d = new Date();
            var n = d.valueOf();
            $("#img_cap").attr("src", "CImage.aspx?" + n + "");
        });
    }
    $('input:radio[name="gender"]').change(
        function() {
			refreshCaptcha();
            if ($(this).is(':checked') && $(this).val() == 'PAN') {
                $("#txtStat").prop("placeholder", "Enter PAN").val("").focus().blur();
                $("#txtStat").attr("maxlength", 10);
                $("#hdnval").val("1");
                $("#txtIfsc").hide();
            }
            if ($(this).is(':checked') && $(this).val() == 'APP') {
                $("#txtStat").prop("placeholder", "Enter Application Number").val("").focus().blur();
                $("#txtStat").attr("maxlength", 25);
                $("#hdnval").val("2");
                $("#txtIfsc").hide();
            }
            if ($(this).is(':checked') && $(this).val() == 'other') {
                $("#txtStat").prop("placeholder", "Enter DPID CLID").val("").focus().blur();
                $("#txtStat").attr("maxlength", 16);
                $("#hdnval").val("3");
                $("#txtIfsc").hide();
            }
            if ($(this).is(':checked') && $(this).val() == 'accountNo') {
                $("#txtStat").prop("placeholder", "Enter Account No").val("").focus().blur();
                $("#txtStat").attr("maxlength", 16);
                $("#txtIfsc").show();
                $("#hdnval").val("4");
            }
    });        
    function ShowCustomDialog(msg) 
    {
        ShowDialogBox('Alert', msg, 'Ok', '', 'GoToAssetList', null);
    }
    function ShowDialogBox(title, content, btn1text, btn2text, functionText, parameterList) {
        var btn1css;
        var btn2css;
        if (btn1text == '') 
        {
            btn1css = "hidecss";
        } 
        else 
        {
            btn1css = "showcss";
        }
        $("#lblMessage").html(content);
        $("#dialog").dialog({
            resizable: false,
            title: title,
            modal: true,
            width: '330px',
            height: 'auto',
            bgiframe: false,
            hide: { effect: 'scale', duration: 400 },
            buttons: [
                        {
                            text: btn1text,
                            "class": btn1css,
                            click: function() 
                            {
                                $("#dialog").dialog('close');
                            }
                        }
                    ]
        });
    }
     function GenToken()
{
  
  $.ajax({
        type: "POST",
        url: "IPO.aspx/generateToken",
        //data: '{cid: "' + $('#ddlCompany').val() + '",mob: "' + $('#mobileNo').val() + '",folio: "' + $('#dpNo').val() + '",email: "' + $('#emailId').val() + '"}',
        contentType: "application/json;charset:utf-8",
        dataType: "json",
        async: false,
        success: function(response) {
            $('#hidToken').val(encVal(response.d));
            //alert(encVal($('#hidToken').val()));
        },
        failure: function(response) {
            alert(response.d);
        },
        error: function(xhr, status, error) {
            var err = eval("(" + xhr.responseText + ")");
            alert(err.Message);
        }
    });
}
function encVal(vl) {
                    var key = CryptoJS.enc.Utf8.parse('8080808080808080');
                    var iv = CryptoJS.enc.Utf8.parse('8080808080808080');

                    var encryptedVal = CryptoJS.AES.encrypt(CryptoJS.enc.Utf8.parse(vl), key,
                    {
                        keySize: 128 / 8,
                        iv: iv,
                        mode: CryptoJS.mode.CBC,
                        padding: CryptoJS.pad.Pkcs7
                    });
               return encryptedVal;
           }
  
    function BindBasicOFAllotment()
    {      
       
//        var url ='Template/'+$('#ddlCompany').val()+'/BasisOfAllotment.pdf'
        var url ="https://linkintime.co.in/Initial_Offer/PDF/"+$('#ddlCompany').val()+"/BasisOfAllotment.pdf";
        
        $.ajax({
        url:url,
        type:'GET',
        error : function(){
        $('#basisOfAllotment').hide();
        },
        success: function(){
        $("#basisOfAllotment").attr('href',url)
        $('#basisOfAllotment').show();
        }
        });
    
    }
    
    
    
